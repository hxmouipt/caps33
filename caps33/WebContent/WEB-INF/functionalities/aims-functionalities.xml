<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN" "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
  <managed-bean>
    <managed-bean-name>aimsMap</managed-bean-name>
    <managed-bean-class>
      com.esri.adf.web.aims.data.AIMSMapFunctionality
    </managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
    <!-- To perform Query and Geocode Functionality set loadFields and loadExtensions property to true. -->
    <managed-property>
      <property-name>loadFields</property-name>
      <value>true</value>
    </managed-property>
    <managed-property>
      <property-name>loadExtensions</property-name>
      <value>true</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>aimsGeocode</managed-bean-name>
    <managed-bean-class>
      com.esri.adf.web.aims.data.AIMSGeocodeFunctionality
    </managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>aimsOverview</managed-bean-name>
    <managed-bean-class>
      com.esri.adf.web.aims.data.AIMSOverviewFunctionality
    </managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>aimsToc</managed-bean-name>
    <managed-bean-class>
      com.esri.adf.web.aims.data.AIMSTocFunctionality
    </managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
    <managed-property>
      <property-name>showSwatches</property-name>
      <value>true</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>aimsQuery</managed-bean-name>
    <managed-bean-class>
      com.esri.adf.web.aims.data.query.AIMSQueryFunctionality
    </managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
  </managed-bean>

</faces-config>
