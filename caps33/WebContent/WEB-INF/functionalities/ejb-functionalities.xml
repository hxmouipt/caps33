<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN" "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
  <managed-bean>
    <managed-bean-name>ejbMap</managed-bean-name>
    <managed-bean-class>
      com.esri.adf.web.ejb.data.EJBMapFunctionality
    </managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>ejbOverview</managed-bean-name>
    <managed-bean-class>
      com.esri.adf.web.ejb.data.EJBOverviewFunctionality
    </managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>ejbToc</managed-bean-name>
    <managed-bean-class>
      com.esri.adf.web.ejb.data.EJBTocFunctionality
    </managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
  </managed-bean>

    <managed-bean>
    <managed-bean-name>ejbGeocode</managed-bean-name>
    <managed-bean-class>
      com.esri.adf.web.ejb.data.EJBGeocodeFunctionality
    </managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>ejbQuery</managed-bean-name>
    <managed-bean-class>
      com.esri.adf.web.ejb.data.query.EJBQueryFunctionality
    </managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
  </managed-bean>

</faces-config>
